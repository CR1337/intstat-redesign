#!/bin/bash

# Name of the container (replace if different)
CONTAINER_NAME="mysql-test-container"

# Check if the container exists
if [ "$(docker ps -a -q -f name="$CONTAINER_NAME" 2>/dev/null)" ]; then
    echo "The container '$CONTAINER_NAME' exists."
    exit 0
else
    echo "The container '$CONTAINER_NAME' does *not* exist."
    exit 1
fi
